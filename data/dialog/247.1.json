            "utt_id": "utt-72",
            "utterance": "Can you repeat the last part?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_repeat;\", \"tracker_completed_step\": \"inst-17\", \"tracker_completed_step_num\": 31, \"tracker_requested_step\": \"inst-17\", \"tracker_requested_step_num\": 31}"
        },
