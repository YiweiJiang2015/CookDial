            "annotations": "{\"section\": \"inst\", \"intent\": \"req_is_recipe_finished;\", \"tracker_completed_step\": \"inst-19\", \"tracker_completed_step_num\": 33, \"tracker_requested_step\": \"inst-19\", \"tracker_requested_step_num\": 33}"
