            "utt_id": "utt-62",
            "utterance": "No problem. We can continue, bot.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-14\", \"tracker_completed_step_num\": 28, \"tracker_requested_step\": \"inst-15\", \"tracker_requested_step_num\": 29}"
        },
        {
            "utt_id": "utt-63",
            "utterance": "Spoon 1/3 cup of chicken mixture onto each tortilla.",
            "bot": true,
            "annotations": "inform_instruction(inst-15);"
        },
