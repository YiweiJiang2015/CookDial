            "utt_id": "utt-74",
            "utterance": "Thanks! Now I understand it. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank; req_instruction;\", \"tracker_completed_step\": \"inst-17\", \"tracker_completed_step_num\": 31, \"tracker_requested_step\": \"inst-18\", \"tracker_requested_step_num\": 32}"
