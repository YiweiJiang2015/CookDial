{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hi. how do I make best macaroni salad?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"greeting; req_start;\", \"tracker_completed_step\": \"ing-0\", \"tracker_completed_step_num\": 1, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi. you will have to prepare 9 ingredients. I will introduce them one by one when you are ready.",
            "bot": true,
            "annotations": "greeting();\r\nsum_ingredient_list();\r\npropose_list_ingredients();"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Ok. I'm ready.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm;\", \"tracker_completed_step\": \"ing-0\", \"tracker_completed_step_num\": 1, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "First take 1 package macaroni and 1/2 cup mayonnaise.",
            "bot": true,
            "annotations": "inform_ingredient(ing-0);\r\ninform_ingredient(ing-1);"
        },
        {
            "utt_id": "utt-4",
            "utterance": "How big should the package be?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "The package should be 16 ounces.",
            "bot": true,
            "annotations": "inform_amount(ing-0);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok. What are the next ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-0\", \"tracker_completed_step_num\": 1, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "Next prepare 1 cup cucumber. The cucumber should be peeled, seeded and chopped.",
            "bot": true,
            "annotations": "inform_ingredient(ing-2);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Ok. I have done that. What is the next one?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-2\", \"tracker_completed_step_num\": 3, \"tracker_requested_step\": \"ing-3\", \"tracker_requested_step_num\": 4}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "Next take 1 tablespoon dried minced onion and 1 cup diced ham.",
            "bot": true,
            "annotations": "inform_ingredient(ing-3);\r\ninform_ingredient(ing-4);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Could you repeat the last ingredient?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_repeat;\", \"tracker_completed_step\": \"ing-4\", \"tracker_completed_step_num\": 5, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "1 cup diced ham.",
            "bot": true,
            "annotations": "repeat(ing-4);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok. what is next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-4\", \"tracker_completed_step_num\": 5, \"tracker_requested_step\": \"ing-5\", \"tracker_requested_step_num\": 6}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "Take 1 cup shredded Cheddar cheese.",
            "bot": true,
            "annotations": "inform_ingredient(ing-5);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ok. I have that as well. Are there more ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient_list_ends;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"ing-6\", \"tracker_requested_step_num\": 7}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "Yes 3 tablespoons olive oil and 3 tablespoons white wine vinegar.",
            "bot": true,
            "annotations": "inform_ingredient(ing-6);\r\ninform_ingredient(ing-7);"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Ok. what is next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-7\", \"tracker_completed_step_num\": 8, \"tracker_requested_step\": \"ing-8\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "Finally take some salt and pepper to taste.",
            "bot": true,
            "annotations": "inform_ingredient(ing-8);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Ok. I have all the ingredients. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"ing-8\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "Take a large pot with salted water. Bring it to a boil and cook the macaroni until al dente.",
            "bot": true,
            "annotations": "inform_instruction(ac-0-0);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "How long should I cook it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_duration;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "Until al dente.",
            "bot": true,
            "annotations": "inform_duration(cond-0-0);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Ok. is there any indication of how many minutes you need to cook until you reach al dente?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_duration;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "I cannot answer that question.",
            "bot": true,
            "annotations": "fence();"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Ok. what is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-25",
            "utterance": "When the pasta is al dente, rinse it under cold water and drain it.",
            "bot": true,
            "annotations": "inform_instruction(ac-0-1);\r\ninform_instruction(ac-0-2);"
        },
        {
            "utt_id": "utt-26",
            "utterance": "Ok. I have done that. What is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 11}"
        },
        {
            "utt_id": "utt-27",
            "utterance": "Take a large bowl and combine the macaroni and mayonnaise. Make sure to mix well.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-28",
            "utterance": "Ok. what should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-29",
            "utterance": "Now you should stir in the cucumber, onion, ham and cheese. Again make sure to mix everything well.",
            "bot": true,
            "annotations": "inform_instruction(inst-2);\r\ninform_instruction(inst-3);"
        },
        {
            "utt_id": "utt-30",
            "utterance": "Ok. I have stirred those ingredients in the bowl. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 13, \"tracker_requested_step\": \"inst-4\", \"tracker_requested_step_num\": 14}"
        },
        {
            "utt_id": "utt-31",
            "utterance": "Drizzle the oil and white wine vinegar over the top and toss the bowl.",
            "bot": true,
            "annotations": "inform_instruction(inst-4);"
        },
        {
            "utt_id": "utt-32",
            "utterance": "Ok. I have done that. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-4\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-5\", \"tracker_requested_step_num\": 15}"
        },
        {
            "utt_id": "utt-33",
            "utterance": "Finally place it in the refrigerator for at least 4 hours.",
            "bot": true,
            "annotations": "inform_instruction(inst-5);"
        },
        {
            "utt_id": "utt-34",
            "utterance": "Ok. I have done that. Is there something else I should still do?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_is_recipe_ongoing;\", \"tracker_completed_step\": \"inst-5\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"inst-5\", \"tracker_requested_step_num\": 15}"
        },
        {
            "utt_id": "utt-35",
            "utterance": "No that's all. Enjoy your macaroni salad.",
            "bot": true,
            "annotations": "negate();\r\nend_recipe();\r\nenjoy();"
        },
        {
            "utt_id": "utt-36",
            "utterance": "Ok. thanks for the instructions.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank;\", \"tracker_completed_step\": \"inst-5\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"inst-5\", \"tracker_requested_step_num\": 15}"
        }
    ]
