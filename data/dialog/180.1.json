{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hello! What are we gonna prepare today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi we will make an easy punch. It will be a short recipe.",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Great! Could I have the ingredients first?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient_list;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Yes, you will need 4 packages unsweetened strawberry-flavored drink mix powder.",
            "bot": true,
            "annotations": "affirm();\r\ninform_ingredient(ing-0);"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Ok, how big are the packages supposed to be?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-0\", \"tracker_completed_step_num\": 1, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "(0.13 ounce. The next ingredients are 2 cans pineapple juice and 3 cups white sugar.",
            "bot": true,
            "annotations": "inform_amount(ing-0);\r\ninform_ingredient(ing-1);\r\ninform_ingredient(ing-2);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok, good, how big should the can be?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-2\", \"tracker_completed_step_num\": 3, \"tracker_requested_step\": \"ing-1\", \"tracker_requested_step_num\": 2}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "It should be 46 fluid ounce. The last two ingredients are 4 quarts water abd 2 liters ginger ale.",
            "bot": true,
            "annotations": "inform_amount(ing-1);\r\ninform_ingredient(ing-3);\r\ninform_ingredient(ing-4);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Ok, good I have those ready. Should we start with the preparations?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"ing-4\", \"tracker_completed_step_num\": 5, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 6}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "Now take a large punch  bowl and combine the drink mix, pineapple juice, sugar and water.",
            "bot": true,
            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Ok I did that! I used all the pineapple juice, is that ok?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_use_all;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 7}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "Yes! Now stir until it is dissolved.",
            "bot": true,
            "annotations": "affirm();\r\ninform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok it looks good now, what next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 7, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 8}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "Finally stir in the ginger ale and enjoy your easy punch!",
            "bot": true,
            "annotations": "inform_instruction(inst-2);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Nice! Can I add ice to it?",
