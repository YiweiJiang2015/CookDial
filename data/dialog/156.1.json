{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hello what are we going to prepare today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi today we will make some delicious chicken tacos.",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);"
        },
        {
            "utt_id": "utt-2",
            "utterance": "That's great! How many ingredients are there in this recipe?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient_list_length;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "It has 14 ingredients. Do you want to prepare all of them beforehand?",
            "bot": true,
            "annotations": "sum_ingredient_list();\r\npropose_list_ingredients();"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Yes please!",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "First you will need 1 pound skinless, boneless chicken breast halves. You should cut them into bite size pieces.",
            "bot": true,
            "annotations": "inform_ingredient(ing-0);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok I did that! what next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-0\", \"tracker_completed_step_num\": 1, \"tracker_requested_step\": \"ing-1\", \"tracker_requested_step_num\": 2}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "The next ingredients are 1 cup lemonade, 2 tablespoons olive oil and 1 tablespoon lime juice.",
            "bot": true,
            "annotations": "inform_ingredient(ing-1);\r\ninform_ingredient(ing-2);\r\ninform_ingredient(ing-3);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Ok, I just have lemon and not lime, is that ok?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"other;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-1\", \"tracker_requested_step_num\": 2}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "Sorry I cannot answer that question.",
            "bot": true,
            "annotations": "fence();"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Ok no problem can I have two more ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "Ok, the next ones are 1 1/2 teaspoons Worcestershire sauce and 1/2 teaspoon garlic powder.",
            "bot": true,
            "annotations": "inform_ingredient(ing-4);\r\ninform_ingredient(ing-5);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok can I have more ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"ing-6\", \"tracker_requested_step_num\": 7}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "Yes 1/2 teaspoon onion powder, 1 bay leaf and 1 package corn tortillas.",
            "bot": true,
            "annotations": "inform_ingredient(ing-6);\r\ninform_ingredient(ing-7);\r\ninform_ingredient(ing-8);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ok how big should the package be?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-8\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"ing-8\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "It should be 12 ounce. The next ingredients are 1 head lettuce. It should be shredded. And 2 large tomatoes, chopped.",
            "bot": true,
            "annotations": "inform_amount(ing-8);\r\ninform_ingredient(ing-9);\r\ninform_ingredient(ing-10);"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Ok good are there more ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-10\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"ing-11\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "Yes there are 3 ingredients left. 1 package shredded sharp Cheddar cheese/ 1 jar salsa and 1 container sour cream.",
            "bot": true,
            "annotations": "inform_ingredient(ing-11);\r\ninform_ingredient(ing-12);\r\ninform_ingredient(ing-13);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Ok can I have the quantities of these ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-13\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"ing-11\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "They are all 8 ounce.",
            "bot": true,
            "annotations": "inform_amount(ing-11);\r\ninform_amount(ing-12);\r\ninform_amount(ing-13);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "Ok good should we start?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"ing-13\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 15}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "Yes let's start. First take a large skillet and put it over medium heat. Now combine chicken, lemonade, olive oil, lime juice, and Worcestershire sauce.",
            "bot": true,
            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Ok I did that, should I use all the lime?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; other;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 16}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "Yes. Now season it with garlic powder, onion powder, and bay leaf.",
            "bot": true,
            "annotations": "affirm();\r\ninform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Ok I did that, what next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 16, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 16}"
        },
        {
            "utt_id": "utt-25",
            "utterance": "Let is simmer for 15 to 20 minutes.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-26",
            "utterance": "Ok 20 minutes passed, what next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 16, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 16}"
        },
        {
            "utt_id": "utt-27",
            "utterance": "Is the chicken no longer pink and are the juices running clear?",
            "bot": true,
            "annotations": "verify(cond-2-0);"
        },
        {
            "utt_id": "utt-28",
            "utterance": "Yes that's it! what next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"affirm; req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 16, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 18}"
        },
        {
            "utt_id": "utt-29",
            "utterance": "Great, now warm the tortillas in the oven or microwave until soft. And transfer the chicken to a serving bowl.",
            "bot": true,
            "annotations": "inform_instruction(inst-3);"
        },
        {
            "utt_id": "utt-30",
            "utterance": "Ok good, for how long should I warm the tortillas in the oven?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_duration;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 18, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 18}"
        },
        {
            "utt_id": "utt-31",
            "utterance": "They are ready when they become soft.",
            "bot": true,
            "annotations": "inform_duration(cond-3-0);"
        },
        {
            "utt_id": "utt-32",
            "utterance": "Ok no problem, the tortillas are warm and the chicken is on a serving bowl, what is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 18, \"tracker_requested_step\": \"inst-5\", \"tracker_requested_step_num\": 20}"
        },
        {
            "utt_id": "utt-33",
            "utterance": "Ok good. Now place lettuce, tomatoes, cheese, salsa, and sour cream in serving dishes.",
            "bot": true,
            "annotations": "inform_instruction(inst-5);"
        },
        {
            "utt_id": "utt-34",
            "utterance": "Ok done! what next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-5\", \"tracker_completed_step_num\": 20, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 21}"
        },
        {
            "utt_id": "utt-35",
            "utterance": "Finally each person can create their own wrap. They can choose their preferred ingredients.",
            "bot": true,
            "annotations": "inform_instruction(inst-6);"
        },
        {
            "utt_id": "utt-36",
            "utterance": "Noyce! so is that it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_is_recipe_finished;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 21, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 21}"
        },
        {
            "utt_id": "utt-37",
            "utterance": "Yes that was all, enjoy the tacos!",
            "bot": true,
            "annotations": "affirm();\r\nend_recipe();\r\nenjoy();"
        },
        {
            "utt_id": "utt-38",
            "utterance": "Thanks!!",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 21, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 21}"
        }
    ]
