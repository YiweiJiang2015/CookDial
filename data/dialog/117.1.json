        {
            "utt_id": "utt-25",
            "utterance": "Good you can pour the mixture into the pans you prepared before.",
            "bot": true,
            "annotations": "inform_instruction(inst-6);"
        },
        {
            "utt_id": "utt-26",
            "utterance": "Ok the mixture is in the pan.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 17, \"tracker_requested_step\": \"inst-7\", \"tracker_requested_step_num\": 18}"
        },
