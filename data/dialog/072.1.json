{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hi. what are we making today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi. we are making classic peanut butter cookies. Are you ready?",
            "bot": true,
            "annotations": "inform_title(title);\r\npropose_start_recipe();"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Yes I am. What's the first step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"affirm; req_instruction;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "First cream together 3 ingredients. The first two are 1 cup unsalted butter and 1 cup crunchy peanut butter.",
            "bot": true,
            "annotations": "simplify_begin(inst-0);\r\nsimplify_continue(inst-0, [ing-0, ing-1]);"
        },
        {
            "utt_id": "utt-4",
            "utterance": "I have done the first two ingredients, what is the third one?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "The third one is 1 cup white sugar. Also add 2 eggs.",
            "bot": true,
            "annotations": "simplify_continue(inst-0, [ing-2]);\r\ninform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Should I add all of the egg?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"other;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "I cannot answer that question.",
            "bot": true,
            "annotations": "fence();"
        },
        {
            "utt_id": "utt-8",
            "utterance": "No problem, what's the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "Now take a separate bowl and we will add 4 other ingredients. The first two are 2 1/2 cups all-purpose flour and 1 teaspoon baking powder.",
            "bot": true,
            "annotations": "simplify_begin(inst-2);\r\nsimplify_continue(inst-2, [ing-5, ing-6]);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "How many cups all-purpose flour?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"inst-2\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"ing-5\", \"tracker_requested_step_num\": 6}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "2 and 1/2 cups.",
            "bot": true,
            "annotations": "inform_amount(ing-5);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Thank you. What are the other two ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank; req_instruction;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "The other ingredients are 1/2 teaspoon salt and 1 1/2 teaspoons baking soda.",
            "bot": true,
            "annotations": "simplify_continue(inst-2, [ing-7, ing-8]);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "What's next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-2\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 13}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "Now stir in the mixture into the batter you made before and put it in the refrigerator for 1 hour.",
            "bot": true,
            "annotations": "inform_instruction(inst-3);\r\ninform_instruction(inst-4);"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Ok. I will do that. Can I do anything in the meantime?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_parallel_action;\", \"tracker_completed_step\": \"inst-4\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-5\", \"tracker_requested_step_num\": 15}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "No just wait. Afterwards you should roll the mixture into 1 inch balls and put them on baking sheets.",
            "bot": true,
            "annotations": "inform_instruction(inst-5);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Ok. I have rolled the mixture into one inch balls and I have placed them on baking sheets. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-5\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 16}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "Now flatten each ball with a fork, making a criss-cross pattern.",
            "bot": true,
            "annotations": "inform_instruction(inst-6);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "I have done that.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 16, \"tracker_requested_step\": \"inst-7\", \"tracker_requested_step_num\": 17}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "Ok. Now preheat the oven to 375 degrees Fahrenheit and bake the cookies for about 10 minutes.",
            "bot": true,
            "annotations": "inform_instruction(inst-7);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "How do I know when they are ready?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_duration;\", \"tracker_completed_step\": \"inst-7\", \"tracker_completed_step_num\": 17, \"tracker_requested_step\": \"inst-8\", \"tracker_requested_step_num\": 18}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "They are ready when the cookies begin to brown. Do not over-bake them.",
            "bot": true,
            "annotations": "inform_duration(cond-7-0);\r\ninform_instruction(inst-8);"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Ok. they are ready now. Should I do anything else?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-8\", \"tracker_completed_step_num\": 18, \"tracker_requested_step\": \"inst-8\", \"tracker_requested_step_num\": 18}"
        },
