    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Good morning, what are we making today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi! Today we are gonna make caramelized apple bites! It's a quick and tasty recipe with 3 ingredients. Are you ready to start?",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);\r\nsum_ingredient_list();\r\npropose_start_recipe();"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Ok yeah. I'm ready.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Good! Would you like to prepare the ingredients first?",
            "bot": true,
            "annotations": "propose_list_ingredients();"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Yes, what are the ingredients?",
