        },
        {
            "utt_id": "utt-35",
            "utterance": "Now take it out of the oven and let it cool on  a wire rack.",
            "bot": true,
            "annotations": "repeat(inst-13);"
        },
        {
            "utt_id": "utt-36",
            "utterance": "Ok good. For how long should I let it cool for?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_duration;\", \"tracker_completed_step\": \"inst-13\", \"tracker_completed_step_num\": 21, \"tracker_requested_step\": \"inst-13\", \"tracker_requested_step_num\": 21}"
        },
