        },
        {
            "utt_id": "utt-25",
            "utterance": "Now add the remaining 1/2 cup chicken broth or water, 2 tablespoons soy sauce, 2 tablespoons white sugar and 2 tablespoons cornstarch.",
            "bot": true,
            "annotations": "inform_instruction(inst-5);"
        },
