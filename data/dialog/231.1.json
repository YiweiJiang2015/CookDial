            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 4}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "Noyce! Take a medium saucepan and combine inside whipping cream, butter or margarine, and grated Parmesan cheese.",
            "bot": true,
            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok the ingredients are inside. What should I do with it?",
