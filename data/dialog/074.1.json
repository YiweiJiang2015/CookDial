        },
        {
            "utt_id": "utt-5",
            "utterance": "Now add 2 tablespoons chopped fresh parsley and 2 tablespoons olive oil.",
            "bot": true,
            "annotations": "simplify_begin(inst-0);\r\nsimplify_continue(inst-0, [ing-0, ing-1]);"
        },
