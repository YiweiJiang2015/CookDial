    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hallo, what are we gonna cook today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi we will make cucumber sandwiches. It will be a short recipe.",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Ok. Great! What are the ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_ingredient_list;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Take a bowl and combine these two ingredients. 1 package cream cheese spread and 2 teaspoons ranch salad dressing mix.",
            "bot": true,
            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Ok. How big should the cheese package be?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 5, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "It should be 8 ounce.",
            "bot": true,
            "annotations": "inform_amount(ing-0);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok. Good. What next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"ing-0\", \"tracker_completed_step_num\": 1, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 6}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "Now take 12 slices pumpernickel rye bread and spread the mixture on one side of each slice of bread.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Sorry I didn't understand which bread I should use.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_repeat;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
