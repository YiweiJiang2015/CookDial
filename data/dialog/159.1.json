        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi, today we are making dilled garlic! A tasty recipe with 5 ingredients! Are you ready?",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);\r\nsum_ingredient_list();\r\npropose_start_recipe();"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Nice, I'm ready.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Great! We can start by preparing the ingredient. The first two are 2 of pounds of garlic and one sprig of chopped fresh dill.",
            "bot": true,
            "annotations": "inform_ingredient(ing-0);\r\ninform_ingredient(ing-1);"
        },
