            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok how large should the containers be?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 6}"
