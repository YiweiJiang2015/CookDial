            "utt_id": "utt-38",
            "utterance": "Alright, all finished! Looks great! Thanks.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; thank;\", \"tracker_completed_step\": \"inst-9\", \"tracker_completed_step_num\": 28, \"tracker_requested_step\": \"inst-9\", \"tracker_requested_step_num\": 28}"
