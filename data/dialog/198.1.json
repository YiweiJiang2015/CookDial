    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hello! What can I eat for breakfast?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi, do you like Banana Chip Muffins?",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);\r\npropose_start_recipe();"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Mmmh I loove them, it's like you read my mind!",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Good. Well maybe in the future I can. The recipe has 9 ingredients. Do you want to prepare all of them beforehand?",
            "bot": true,
            "annotations": "sum_ingredient_list();\r\npropose_list_ingredients();"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Ahah I hope I'll be dead by then! Yes I'd like to prepare the ingredients first!",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "Good, the first two recipes are 1 and 3/4 cups all-purpose flour. And 1/2 cup white sugar.",
            "bot": true,
            "annotations": "inform_ingredient(ing-0);\r\ninform_ingredient(ing-1);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok, can I use stevia instead of white sugar?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_substitute;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "Sorry I cannot answer that question. Hi the next two ingredients are 1 tablespoon baking powder and 1/2 teaspoon salt.",
            "bot": true,
            "annotations": "fence();\r\ninform_ingredient(ing-2);\r\ninform_ingredient(ing-3);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Ok, I have baking powder and salt ready, what should I prepare next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "Next is 1/2 cup semisweet chocolate chips, 1 egg and 1/4 cup vegetable oil.",
            "bot": true,
            "annotations": "inform_ingredient(ing-4);\r\ninform_ingredient(ing-5);\r\ninform_ingredient(ing-6);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Ok, oil and chocolate chip are also ready on my counter. Are there any more ingredients? And if so how many?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-6\", \"tracker_completed_step_num\": 7, \"tracker_requested_step\": \"ing-7\", \"tracker_requested_step_num\": 8}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "Good, yes there are 2 ingredients left. 1/4 cup milk and 1 cup mashed bananas.",
            "bot": true,
            "annotations": "affirm();\r\ninform_ingredient(ing-7);\r\ninform_ingredient(ing-8);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok, good, do you think it's ok if I replace the milk with coconut milk?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_substitute;\", \"tracker_completed_step\": \"ing-8\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"ing-7\", \"tracker_requested_step_num\": 8}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "Sorry I cannot answer that question.",
            "bot": true,
            "annotations": "fence();"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ok, no problem! So we can start with the recipe?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"ing-8\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 10}"
        },
