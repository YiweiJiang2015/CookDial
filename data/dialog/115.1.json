{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hi I want a dessert. What are we making today.",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi! If you want a dessert, we can cook today a chocolate cream dessert!",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Hm that sounds perfect.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"other;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "This recipe has 7 ingredients, do you want to hear them first?",
            "bot": true,
            "annotations": "sum_ingredient_list();\r\npropose_list_ingredients();"
        },
        {
            "utt_id": "utt-4",
            "utterance": "How should I start? Yes, give me the ingredients.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "The first two ingredients are 1 fourth of a cup of cold margarine and 1 cup all-purpose flour.",
            "bot": true,
            "annotations": "inform_ingredient(ing-0);\r\ninform_ingredient(ing-1);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok. I have them.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "Good the next two ingredients are 1 package light cream cheese, softened and 2 tablespoons heat-stable granular sugar substitute.",
            "bot": true,
            "annotations": "inform_ingredient(ing-2);\r\ninform_ingredient(ing-3);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Could you repeat the second ingredient?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_repeat;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-3\", \"tracker_requested_step_num\": 4}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "The second ingredient is 2 tablespoons of heat-stable granular sugar substitute.",
            "bot": true,
            "annotations": "repeat(ing-3);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Could I also use normal sugar?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_substitute;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-3\", \"tracker_requested_step_num\": 4}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "Sorry I cannot reply to your question :(.",
            "bot": true,
            "annotations": "fence();"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok, what are the next ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "Next two ingredients are one container reduced-fat frozen whipped topping and one and a half cup of cold skim milk.",
            "bot": true,
            "annotations": "inform_ingredient(ing-4);\r\ninform_ingredient(ing-5);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Sorry, could you repeat them once one by one.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_repeat;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "Sure, no problem. The first ingredient is reduced-fat frozen whipped topping. The quantity is one container.",
            "bot": true,
            "annotations": "repeat(ing-4);"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Ok. How large should the container be?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-4\", \"tracker_completed_step_num\": 5, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "It should an 8  ounces container. The second ingredient is cold skim milk. The quantity is one and a half cup.",
            "bot": true,
            "annotations": "inform_amount(ing-4);\r\ninform_amount(ing-5);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Ok. Thanks. I have both of them. What are the next ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"thank; confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"ing-6\", \"tracker_requested_step_num\": 7}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "There is one more ingredient and that is one package instant sugar-free chocolate pudding mix.",
            "bot": true,
            "annotations": "inform_ingredient(ing-6);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "Ok. How large should the package be?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-6\", \"tracker_completed_step_num\": 7, \"tracker_requested_step\": \"ing-6\", \"tracker_requested_step_num\": 7}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "It should be a 1.4 ounces package. Are you ready to start with the recipe?",
            "bot": true,
            "annotations": "inform_amount(ing-6);\r\npropose_start_instruction();"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Ok. I have all the ingredients. Yes I'm ready.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; affirm;\", \"tracker_completed_step\": \"ing-6\", \"tracker_completed_step_num\": 7, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 8}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "Good! let's start! Take a bowl and cut margarine into flour until it becomes crumbly.",
            "bot": true,
            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Ok. It's crumbly now.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 8, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-25",
            "utterance": "Great! Now press into an 11-in baking dish coated with nonstick cooking spray.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-26",
            "utterance": "Ok. I have done that.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-27",
            "utterance": "Great! Now Bake at 350 degrees F for 15-18 minutes or until lightly browned. After that, cool completely.",
            "bot": true,
            "annotations": "inform_instruction(inst-2);\r\ninform_instruction(inst-3);"
        },
        {
            "utt_id": "utt-28",
            "utterance": "Ok. Do you also have the temperature in Celsius?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_temperature;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-29",
            "utterance": "Sorry it's beyond my ability.",
            "bot": true,
            "annotations": "fence();"
        },
        {
            "utt_id": "utt-30",
            "utterance": "Ok. No worries. It's cooling down now.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-4\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-31",
            "utterance": "Good, when it's completely cool, take a mixing bowl and beat cream cheese and sweetener until smooth.",
            "bot": true,
            "annotations": "inform_instruction(inst-4);"
        },
        {
            "utt_id": "utt-32",
            "utterance": "Ok. The cream is smooth. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-4\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"inst-5\", \"tracker_requested_step_num\": 13}"
        },
        {
            "utt_id": "utt-33",
            "utterance": "Good, now Fold in half of the whipped topping.",
            "bot": true,
            "annotations": "inform_instruction(inst-5);"
        },
        {
            "utt_id": "utt-34",
            "utterance": "Ok. I have done that.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-5\", \"tracker_completed_step_num\": 13, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 14}"
        },
        {
            "utt_id": "utt-35",
            "utterance": "Good. You can now Carefully spread the mixture over the crust.",
            "bot": true,
            "annotations": "inform_instruction(inst-6);"
        },
        {
            "utt_id": "utt-36",
            "utterance": "Ok I have done that. Is there more to do or can I eat it already?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_is_recipe_ongoing;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-7\", \"tracker_requested_step_num\": 15}"
        },
        {
            "utt_id": "utt-37",
            "utterance": "No, there are few more steps before you can eat the dessert. Be strong and you will enjoy it very soon! Now take a mixing bowl and ombine milk and pudding mix.",
            "bot": true,
            "annotations": "negate();\r\ninform_instruction(inst-7);"
        },
        {
            "utt_id": "utt-38",
            "utterance": "Ok. I have done that. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-7\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"inst-8\", \"tracker_requested_step_num\": 16}"
        },
        {
            "utt_id": "utt-39",
            "utterance": "Next is to Beat on low speed for 2 minutes.",
            "bot": true,
            "annotations": "inform_instruction(inst-8);"
        },
        {
            "utt_id": "utt-40",
            "utterance": "Ok. 2 minutes has passed.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-8\", \"tracker_completed_step_num\": 16, \"tracker_requested_step\": \"inst-9\", \"tracker_requested_step_num\": 17}"
        },
        {
            "utt_id": "utt-41",
            "utterance": "Great! Now let it stand for 2-3 minutes.",
            "bot": true,
            "annotations": "inform_instruction(inst-9);"
        },
        {
            "utt_id": "utt-42",
            "utterance": "Ok. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-9\", \"tracker_completed_step_num\": 17, \"tracker_requested_step\": \"inst-10\", \"tracker_requested_step_num\": 18}"
        },
        {
            "utt_id": "utt-43",
            "utterance": "You now spread over the cream cheese and top with the remaining whipped topping. Once you are done you'll be ready to enjoy your dessert! :).",
            "bot": true,
            "annotations": "inform_instruction(inst-10);\r\ninform_instruction(inst-11);\r\nend_recipe();"
        },
        {
            "utt_id": "utt-44",
            "utterance": "Ok. I have done that. It looks delicious. Thank you!",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; thank;\", \"tracker_completed_step\": \"inst-11\", \"tracker_completed_step_num\": 19, \"tracker_requested_step\": \"inst-10\", \"tracker_requested_step_num\": 18}"
        },
        {
            "utt_id": "utt-45",
            "utterance": "You are welcome! Enjoy!!",
            "bot": true,
            "annotations": "thank();\r\nenjoy();"
        }
    ]
}