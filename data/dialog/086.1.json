{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hello Bot, what are we gonna cook today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Today we will make garlic crab artichoke dip. It will be a short recipe. Are you ready to start?",
            "bot": true,
            "annotations": "inform_title(title);\r\npropose_start_recipe();"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Yes, I am ready! Could I have all the ingredients first so I can prepare them?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm; req_ingredient_list;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Sure. the first two ingredients are 1 cup mayonnaise and 8 ounces sour cream.",
            "bot": true,
            "annotations": "affirm();\r\ninform_ingredient(ing-0);\r\ninform_ingredient(ing-1);"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Good. They are ready. Could I have the next two ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "1 package garlic and mushroom soup mix and 1 can artichoke hearts, drained and chopped.",
            "bot": true,
            "annotations": "inform_ingredient(ing-2);\r\ninform_ingredient(ing-3);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Could you repeat the first ingredient?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_repeat;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "1 package of garlic and mushroom soup mix.",
            "bot": true,
            "annotations": "repeat(ing-2);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Ok, how big the package should be?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-2\", \"tracker_completed_step_num\": 3, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "It should be a 1 ounce package.",
            "bot": true,
            "annotations": "inform_amount(ing-2);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Ok perfect. Could I have the next two ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-2\", \"tracker_completed_step_num\": 3, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "1 cup shredded Swiss cheese and 1 can crab meat. It should be drained.",
            "bot": true,
            "annotations": "inform_ingredient(ing-4);\r\ninform_ingredient(ing-5);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok. How big should the can be?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"ing-5\", \"tracker_requested_step_num\": 6}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "It should be 6 ounces.",
            "bot": true,
            "annotations": "inform_amount(ing-5);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ok. good. Are there more ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_ingredient_list_ends;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 7}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "No, these were all the ingredients. You may now preheat the oven to 175 degrees Celsius.",
            "bot": true,
            "annotations": "negate();\r\ninform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Ok. Good. The oven is 175 degrees now,what next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 7, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 8}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "Take a 1-1/2 quart casserole dish and combine the ingredients you prepared before.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Sorry, I didn't understand. Which kind of casserole should I use?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_tool;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 8, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 8}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "It should be a casserole dish. The size should be 1-1/2 quart.",
            "bot": true,
            "annotations": "inform_tool(tool-1-0);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "Sorry, I didn't understand the size of the casserole.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_tool;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 8, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 8}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "It means one and a half quart. This is a specific size which cannot be pronounced in centimeters.",
            "bot": true,
            "annotations": "inform_tool(tool-1-0);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Ok. Thank you. What should I do next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank; req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 8, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "Bake it uncovered for 30 minutes in the oven.",
            "bot": true,
            "annotations": "inform_instruction(inst-2);"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Ok, 30 minutes passed. How do I see that it's ready?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_duration;\", \"tracker_completed_step\": \"inst-2\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-25",
            "utterance": "I cannot answer that question. After the 30 minutes you should serve it immediately.",
            "bot": true,
            "annotations": "fence();\r\ninform_instruction(inst-3);"
        },
        {
            "utt_id": "utt-26",
            "utterance": "Ook good. Is that it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_is_recipe_finished;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-27",
            "utterance": "Yes, that was all! Enjoy your garlic crab artichoke dip.",
            "bot": true,
            "annotations": "affirm();\r\nend_recipe();\r\nenjoy();"
        },
        {
            "utt_id": "utt-28",
            "utterance": "Thank you!!",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 10}"
        }
    ]
