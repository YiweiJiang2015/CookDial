        },
        {
            "utt_id": "utt-11",
            "utterance": "Next add 1/2 teaspoon salt and 1/4 teaspoon ground turmeric.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "OK. what is next?",
