{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hi, what is today's recommendation?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Today we are making a Coconut Ambrosia Salad.",
            "bot": true,
            "annotations": "inform_title(title);"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Great. What shall I prepare for this salad?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient_list;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "First you need 1 can or 11 ounce of mandarin oranges, drained.",
            "bot": true,
            "annotations": "inform_ingredient(ing-0);"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Can you tell me its weight?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-0\", \"tracker_completed_step_num\": 1, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "11 ounce.",
            "bot": true,
            "annotations": "inform_amount(ing-0);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok. Anything else? Like any sauce?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-0\", \"tracker_completed_step_num\": 1, \"tracker_requested_step\": \"ing-1\", \"tracker_requested_step_num\": 2}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "Secondly, you need 1 can or 8 ounce of crushed pineapple, drained.",
            "bot": true,
            "annotations": "inform_ingredient(ing-1);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Good. Also 11 ounce?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"ing-1\", \"tracker_requested_step_num\": 2}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "No, 8 ounce.",
            "bot": true,
            "annotations": "negate();\r\ninform_amount(ing-1);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Thanks. Next thing is?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"thank; req_ingredient;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "3 and 1/2 cups frozen whipped topping, thawed.",
            "bot": true,
            "annotations": "inform_ingredient(ing-2);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "I got that. Next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-2\", \"tracker_completed_step_num\": 3, \"tracker_requested_step\": \"ing-3\", \"tracker_requested_step_num\": 4}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "2 cups shredded coconut.",
            "bot": true,
            "annotations": "inform_ingredient(ing-3);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ok. Is it all?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient_list_ends;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "No, next you need is 2 cups of miniature marshmallows.",
            "bot": true,
            "annotations": "negate();\r\ninform_ingredient(ing-4);"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Ok. I have it now. Go on, pls.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-4\", \"tracker_completed_step_num\": 5, \"tracker_requested_step\": \"ing-5\", \"tracker_requested_step_num\": 6}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "You still need 1/2 cup of milk and 1 cup of maraschino cherries.",
            "bot": true,
            "annotations": "inform_ingredient(ing-5);\r\ninform_ingredient(ing-6);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Ok. A lot of things on board. Can I start now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"ing-6\", \"tracker_completed_step_num\": 7, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 8}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "Yes. Then, In a large bowl, combine the oranges, pineapple, whipped topping, coconut, marshmallows and milk.",
            "bot": true,
            "annotations": "affirm();\r\ninform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "Ok. I add them to the bowl. What else?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 8, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "Mix together well and chill 1 hour before serving.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Ohh. 1 hour passed. Can I eat it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_is_recipe_finished;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "Garnish with cherries. Enjoy!",
            "bot": true,
            "annotations": "inform_instruction(inst-2);\r\nenjoy();"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Ok. Thanks!",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank;\", \"tracker_completed_step\": \"inst-2\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-25",
            "utterance": "You are welcome!",
            "bot": true,
            "annotations": "thank();"
        }
    ]
