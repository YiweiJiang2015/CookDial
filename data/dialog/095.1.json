        },
        {
            "utt_id": "utt-13",
            "utterance": "You can now Stuff fig halves with goat cheese.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ok. I have done that. What is next?",
