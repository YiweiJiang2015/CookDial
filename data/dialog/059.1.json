        },
        {
            "utt_id": "utt-5",
            "utterance": "Next add 1 teaspoon sugar and 1/2 teaspoon salt.",
            "bot": true,
            "annotations": "simplify_continue(ac-0-0, [ing-2, ing-3]);"
        },
        {
            "utt_id": "utt-6",
