    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hi, do you know how I can make the best tuna salad ever? I bet you do.",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_start;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "HEY! Of course I know! I am the best bot ever and I'll teach you how to make the Best Ever Tuna Salad. Are you ready to be the best cook in the world?",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);\r\npropose_start_recipe();"
        },
        {
            "utt_id": "utt-2",
            "utterance": "It's going to be great, it will be amazing, I just know it, in fact it will be. I'm ready.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Great! I have the feeling it's gonna be tremendous. There are 11 ingredients in this recipe, the first two are 2 eggs and 1 (12 ounce) can of white tuna.",
            "bot": true,
            "annotations": "sum_ingredient_list();\r\ninform_ingredient(ing-0);\r\ninform_ingredient(ing-1);"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Ok I have both ready. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "Next two are 1 (6 ounce) can crabmeat, shredded and half a cup mayonnaise.",
            "bot": true,
            "annotations": "inform_ingredient(ing-2);\r\ninform_ingredient(ing-3);\r\n"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok can I use light mayonnaise in stead?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_substitute;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "Sorry I cannot reply specifically to your question. You can always change ingredients following you intuition!",
            "bot": true,
            "annotations": "fence();\r\n"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Ok no problem. What are the next ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "The next two ingredients are 1 teaspoon sour cream and 1 teaspoon yellow mustard.",
            "bot": true,
            "annotations": "inform_ingredient(ing-4);\r\ninform_ingredient(ing-5);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Ok I have them ready.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"ing-6\", \"tracker_requested_step_num\": 7}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "Good! Next two are half a teaspoon dried dill weed and one eight teaspoon lemon pepper.",
            "bot": true,
            "annotations": "inform_ingredient(ing-6);\r\ninform_ingredient(ing-7);\r\n"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok ready as well. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-7\", \"tracker_completed_step_num\": 8, \"tracker_requested_step\": \"ing-8\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "Next two are 2 tablespoons dill pickle relish and 2 tablespoons sweet pickle relish.",
            "bot": true,
            "annotations": "inform_ingredient(ing-8);\r\ninform_ingredient(ing-9);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ok I have them ready.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"ing-9\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"ing-10\", \"tracker_requested_step_num\": 11}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "Good! Next one is 3/4 cup finely chopped onion.",
            "bot": true,
            "annotations": "inform_ingredient(ing-10);\r\n"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Ok. Are there more ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-10\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"ing-10\", \"tracker_requested_step_num\": 11}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "No, that is it with the ingredients! When you are ready, we can start!",
            "bot": true,
            "annotations": "negate();\r\npropose_start_instruction();"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Ok, I'm ready to start making the best salad ever.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"ing-10\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "Coool! You can Place the eggs in a saucepan and cover them with cold water.",
            "bot": true,
            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "Ok I have done that. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 13}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "Good! Bring the water to a boil and immediately remove from heat.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Ok I removed it from heat. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 13, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 14}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "You now Cover and let eggs stand in hot water for 10 to 12 minutes.",
            "bot": true,
            "annotations": "inform_instruction(inst-2);"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Ok. Is there something I can do in the meantime?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_parallel_action;\", \"tracker_completed_step\": \"inst-2\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 15}"
