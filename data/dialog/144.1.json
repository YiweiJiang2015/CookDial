{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hello! What are we gonna cook today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi, we will make baked apple oatmeal.",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Great!! Is it a long recipe?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient_list_length;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "No, it will be a short recipe with 10 ingredients.",
            "bot": true,
            "annotations": "negate();\r\nsum_ingredient_list();"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Ok good! Can you start to tell me which ingredients I should prepare?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient_list;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "Sure, the first ingredients are 4 cups milk and 1/2 cup packed brown sugar.",
            "bot": true,
            "annotations": "inform_ingredient(ing-0);\r\ninform_ingredient(ing-1);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok good. Could I use white sugar instead of brown sugar?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_substitute;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "Sorry I cannot answer that question. The next ingredients are 2 teaspoons butter or margarine and 1/2 teaspoon salt.",
            "bot": true,
            "annotations": "fence();\r\ninform_ingredient(ing-2);\r\ninform_ingredient(ing-3);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Ok. I have them ready. What are the next ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "The next ingredients are 1/2 teaspoon ground cinnamon and 2 cups old-fashioned oats.",
            "bot": true,
            "annotations": "inform_ingredient(ing-4);\r\ninform_ingredient(ing-5);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Is the second ingredient old fashion oats? Is that a brand?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_repeat;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"ing-5\", \"tracker_requested_step_num\": 6}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "It is a type of oats.",
            "bot": true,
            "annotations": "repeat(ing-5);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok good. Can I have more ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-5\", \"tracker_completed_step_num\": 6, \"tracker_requested_step\": \"ing-6\", \"tracker_requested_step_num\": 7}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "Yes 2 cups chopped, peeled apples and 1 cup chopped walnuts.",
            "bot": true,
            "annotations": "inform_ingredient(ing-6);\r\ninform_ingredient(ing-7);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ok. good! What next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-7\", \"tracker_completed_step_num\": 8, \"tracker_requested_step\": \"ing-8\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "The last two ingredients are 1 cup raisins and 1 cup wheat germ.",
            "bot": true,
            "annotations": "inform_ingredient(ing-8);\r\ninform_ingredient(ing-9);\r\n"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Sorry I didn't understand the second ingredient.",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_repeat;\", \"tracker_completed_step\": \"ing-9\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"ing-9\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "It is 1 cup of wheat germ. Wheat germ is part of a wheat kernel and is responsible for helping the plant reproduce and spawn new wheat.",
            "bot": true,
            "annotations": "repeat(ing-9);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Ok good, thanks for that. So we can start with the recipe?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank; req_instruction;\", \"tracker_completed_step\": \"ing-9\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 11}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "Now we can start with the recipe. Take a saucepan and heat the milk, brown sugar, butter, salt and cinnamon.",
            "bot": true,
            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "Ok, I am heating those ingredients in the saucepan. For how long should I do that?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_duration;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "Sorry I cannot answer that question. Now add the remaining ingredients and mix gently.",
            "bot": true,
            "annotations": "fence();\r\ninform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Ok good. I mixed all the ingredients! What next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 13}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "Spoon the mixture into a greased 2-qt. casserole.",
            "bot": true,
            "annotations": "inform_instruction(inst-2);"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Ok, the mixture is in the casserole.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-2\", \"tracker_completed_step_num\": 13, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 14}"
        },
        {
            "utt_id": "utt-25",
            "utterance": "Good, now finally cover it and bake at 350 degrees Fahrenheit for 45 minutes.",
            "bot": true,
            "annotations": "inform_instruction(inst-3);"
        },
        {
            "utt_id": "utt-26",
            "utterance": "Ok good. I baked it for 45 minutes. How do I see that it's ready?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_duration;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 14}"
        },
        {
            "utt_id": "utt-27",
            "utterance": "You should bake it for 45 minutes.",
            "bot": true,
            "annotations": "inform_duration(dur-3-0);"
        },
        {
            "utt_id": "utt-28",
            "utterance": "Ok no problem! 45 minutes passed! Can I eat it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_is_recipe_finished;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 14}"
        },
        {
            "utt_id": "utt-29",
            "utterance": "Yes, enjoy!",
            "bot": true,
            "annotations": "affirm();\r\nenjoy();"
        },
        {
            "utt_id": "utt-30",
            "utterance": "Good it looks great. Thank you.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 14}"
