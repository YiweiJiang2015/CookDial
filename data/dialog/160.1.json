    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hello bot! What are we going to prepare today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi! we will prepare Elizabeths extreme chocolate lovers cake. Are you an extreme chocolate lover?",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);\r\npropose_start_recipe();"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Of course I am!! How many ingredients does this recipe have?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"affirm; req_ingredient_list_length;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Great. It will be a long recipe with 19 ingredients.",
            "bot": true,
            "annotations": "sum_ingredient_list();"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Wow! Could you tell me the ingredients first?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient_list;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"ing-0\", \"tracker_requested_step_num\": 1}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "Yes. The first two ingredients are 2 1 fourth cups all-purpose flour and 2 teaspoons baking soda.",
            "bot": true,
            "annotations": "affirm();\r\ninform_ingredient(ing-0);\r\ninform_ingredient(ing-1);\r\n"
        },
        {
            "utt_id": "utt-6",
            "utterance": "OK good, what are the next ones?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-1\", \"tracker_completed_step_num\": 2, \"tracker_requested_step\": \"ing-2\", \"tracker_requested_step_num\": 3}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "1/2 teaspoon salt and 6 (1 ounce) squares unsweetened chocolate, chopped.",
            "bot": true,
            "annotations": "inform_ingredient(ing-2);\r\ninform_ingredient(ing-3);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Okay good I have those ready. What next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"ing-4\", \"tracker_requested_step_num\": 5}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "Good next is 1/2 cup unsalted butter, 2 1/2 cups dark brown sugar and 3 eggs.",
            "bot": true,
            "annotations": "inform_ingredient(ing-4);\r\ninform_ingredient(ing-5);\r\ninform_ingredient(ing-6);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "OK good, I also have those ready. What are the next ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"confirm; req_ingredient;\", \"tracker_completed_step\": \"ing-6\", \"tracker_completed_step_num\": 7, \"tracker_requested_step\": \"ing-7\", \"tracker_requested_step_num\": 8}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "1 and 1/2 tablespoons vanilla extract, 1 cup sour cream and 1 cup water.",
            "bot": true,
            "annotations": "inform_ingredient(ing-7);\r\ninform_ingredient(ing-8);\r\ninform_ingredient(ing-9);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Good, what next?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-9\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"ing-10\", \"tracker_requested_step_num\": 11}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "6 (1 ounce) squares unsweetened chocolate, chopped and 1/4 cup unsalted butter.",
            "bot": true,
            "annotations": "inform_ingredient(ing-10);\r\ninform_ingredient(ing-11);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Okay good, could I use margarine instead of butter?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_substitute;\", \"tracker_completed_step\": \"ing-11\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"ing-10\", \"tracker_requested_step_num\": 11}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "Sorry I cannot answer that question.",
            "bot": true,
            "annotations": "fence();"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Okay no problem, tell me the next ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient;\", \"tracker_completed_step\": \"ing-11\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"ing-12\", \"tracker_requested_step_num\": 13}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "1 egg yolk , 4 cups confectioners' sugar and  2 tablespoons vanilla extract.",
            "bot": true,
            "annotations": "inform_ingredient(ing-12);\r\ninform_ingredient(ing-13);\r\ninform_ingredient(ing-14);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Good, are the ingredients over?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_ingredient_list_ends;\", \"tracker_completed_step\": \"ing-14\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"ing-15\", \"tracker_requested_step_num\": 16}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "Not yet there are 4 more. The next two are 2 tablespoons heavy cream and 1 package cream cheese, softened.",
            "bot": true,
            "annotations": "negate();\r\ninform_ingredient(ing-15);\r\ninform_ingredient(ing-16);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "Okay good, how big should be cream cheese package?",
            "bot": false,
            "annotations": "{\"section\": \"ing\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"ing-16\", \"tracker_completed_step_num\": 17, \"tracker_requested_step\": \"ing-16\", \"tracker_requested_step_num\": 17}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "It should be 8 ounce. The last two ingredients are 2 cups bittersweet chocolate chips and 1 cup heavy cream.",
            "bot": true,
            "annotations": "inform_amount(ing-16);\r\ninform_ingredient(ing-17);\r\ninform_ingredient(ing-18);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Okay good, what's next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"ing-18\", \"tracker_completed_step_num\": 19, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 20}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "Now let's start with the recipe. First already preheat the oven to 175 degrees Celsius.",
            "bot": true,
            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Okay, I did that. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 20, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 21}"
        },
        {
            "utt_id": "utt-25",
            "utterance": "You may grease and flour 2 (9 inch) pans.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-26",
            "utterance": "Hey, I did that. What is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 21, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 22}"
        },
        {
            "utt_id": "utt-27",
            "utterance": "Sift together the flour, baking soda and salt and then set aside.",
            "bot": true,
            "annotations": "inform_instruction(inst-2);\r\ninform_instruction(inst-3);"
        },
        {
            "utt_id": "utt-28",
            "utterance": "Okay, good.should I use all the flour?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"other;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 23, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 22}"
        },
        {
            "utt_id": "utt-29",
            "utterance": "Yes.",
            "bot": true,
            "annotations": "affirm();"
        },
        {
            "utt_id": "utt-30",
            "utterance": "Okay, good. What next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 23, \"tracker_requested_step\": \"inst-4\", \"tracker_requested_step_num\": 24}"
        },
        {
            "utt_id": "utt-31",
            "utterance": "Now take a double boiler and heat 6 ounces of chocolate. Keep stirring occasionally, until chocolate becomes melted and smooth. Afterwards, remove from heat and allow to cool to lukewarm.",
            "bot": true,
            "annotations": "inform_instruction(inst-4);\r\ninform_instruction(inst-5);"
        },
        {
            "utt_id": "utt-32",
            "utterance": "Okay, the chocolate looks smooth, and I removed it from the heat. What should I do with it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-5\", \"tracker_completed_step_num\": 25, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 26}"
        },
        {
            "utt_id": "utt-33",
            "utterance": "Nothing for now. Take a large bowl and cream together the butter and brown sugar until it becomes light and fluffy.",
            "bot": true,
            "annotations": "negate();\r\ninform_instruction(inst-6);"
        },
        {
            "utt_id": "utt-34",
            "utterance": "Okay, it looks fluffy now. What should I do with it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 26, \"tracker_requested_step\": \"inst-7\", \"tracker_requested_step_num\": 27}"
        },
        {
            "utt_id": "utt-35",
            "utterance": "Good, now beat in the eggs one at a time. Then stir in 1 and 1/2 teaspoon vanilla.",
            "bot": true,
            "annotations": "inform_instruction(inst-7);\r\n"
        },
        {
            "utt_id": "utt-36",
            "utterance": "Okay I did that. What is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-7\", \"tracker_completed_step_num\": 27, \"tracker_requested_step\": \"inst-8\", \"tracker_requested_step_num\": 28}"
        },
        {
            "utt_id": "utt-37",
            "utterance": "Good. Now stir in the melted chocolate.",
            "bot": true,
            "annotations": "inform_instruction(inst-8);"
        },
        {
            "utt_id": "utt-38",
            "utterance": "Okay, the chocolate is in. What is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-8\", \"tracker_completed_step_num\": 28, \"tracker_requested_step\": \"inst-9\", \"tracker_requested_step_num\": 29}"
        },
        {
            "utt_id": "utt-39",
            "utterance": "Nice, now beat in the flour mixture alternately with the sour cream.",
            "bot": true,
            "annotations": "inform_instruction(inst-9);"
        },
        {
            "utt_id": "utt-40",
            "utterance": "Ok, I am adding flour mixture and sour cream. Should I put them together at the same time?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; other;\", \"tracker_completed_step\": \"inst-9\", \"tracker_completed_step_num\": 29, \"tracker_requested_step\": \"inst-9\", \"tracker_requested_step_num\": 29}"
        },
        {
            "utt_id": "utt-41",
            "utterance": "No you should alternate the two.",
            "bot": true,
            "annotations": "negate();"
        },
        {
            "utt_id": "utt-42",
            "utterance": "Ok, good. What is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-9\", \"tracker_completed_step_num\": 29, \"tracker_requested_step\": \"inst-10\", \"tracker_requested_step_num\": 30}"
        },
        {
            "utt_id": "utt-43",
            "utterance": "Now stir in the boiling water. The batter will become thin.",
            "bot": true,
            "annotations": "inform_instruction(inst-10);"
        },
        {
            "utt_id": "utt-44",
            "utterance": "Ok, the water is inside now, what should I do next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-10\", \"tracker_completed_step_num\": 30, \"tracker_requested_step\": \"inst-11\", \"tracker_requested_step_num\": 31}"
        },
        {
            "utt_id": "utt-45",
            "utterance": "Great. Now pour this batter into the prepared pans.",
            "bot": true,
            "annotations": "inform_instruction(inst-11);"
        },
        {
            "utt_id": "utt-46",
            "utterance": "Hey, I put it in.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-11\", \"tracker_completed_step_num\": 31, \"tracker_requested_step\": \"inst-12\", \"tracker_requested_step_num\": 32}"
        },
        {
            "utt_id": "utt-47",
            "utterance": "Good. Now bake it in the preheated oven for 35 to 40 minutes.",
            "bot": true,
            "annotations": "inform_instruction(inst-12);"
        },
        {
            "utt_id": "utt-48",
            "utterance": "Okay, 30 minutes past. How do I see that it is ready?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_duration;\", \"tracker_completed_step\": \"inst-12\", \"tracker_completed_step_num\": 32, \"tracker_requested_step\": \"inst-12\", \"tracker_requested_step_num\": 32}"
        },
        {
            "utt_id": "utt-49",
            "utterance": "It will be ready when you can insert a toothpick into the center of the cake and it comes out clean : ).",
            "bot": true,
            "annotations": "inform_duration(cond-12-0);"
        },
        {
            "utt_id": "utt-50",
            "utterance": "Ok, now it looks good! Is it ready?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_is_recipe_finished;\", \"tracker_completed_step\": \"inst-12\", \"tracker_completed_step_num\": 32, \"tracker_requested_step\": \"inst-13\", \"tracker_requested_step_num\": 33}"
        },
        {
            "utt_id": "utt-51",
            "utterance": "Good, yes take it out and let it cool in the pan for 10 minutes. Then turn out onto a wire rack and let it cool completely.",
            "bot": true,
            "annotations": "inform_instruction(inst-13);"
        },
        {
            "utt_id": "utt-52",
            "utterance": "Ok it's cool now! Is it ready to eat?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_is_recipe_finished;\", \"tracker_completed_step\": \"inst-13\", \"tracker_completed_step_num\": 33, \"tracker_requested_step\": \"inst-13\", \"tracker_requested_step_num\": 33}"
        },
        {
            "utt_id": "utt-53",
            "utterance": "Good. No, not yet.",
            "bot": true,
            "annotations": "negate();"
        },
        {
            "utt_id": "utt-54",
            "utterance": "Good, what is the next step then?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-14\", \"tracker_completed_step_num\": 34, \"tracker_requested_step\": \"inst-14\", \"tracker_requested_step_num\": 34}"
        },
        {
            "utt_id": "utt-55",
            "utterance": "We have to prepare the Chocolate Buttercream Frosting. In the top of a double boiler, heat 6 ounces chocolate, stirring occasionally, until chocolate is melted and smooth.",
            "bot": true,
            "annotations": "inform_instruction(inst-14);"
        },
        {
            "utt_id": "utt-56",
            "utterance": "Ok the chocolate looks smooth now.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-14\", \"tracker_completed_step_num\": 34, \"tracker_requested_step\": \"inst-15\", \"tracker_requested_step_num\": 35}"
        },
        {
            "utt_id": "utt-57",
            "utterance": "Take  a large bowl and whip the butter for about 10 minutes until it is super fluffy, beat in egg yolk as well.",
            "bot": true,
            "annotations": "inform_instruction(inst-15);"
        },
        {
            "utt_id": "utt-58",
            "utterance": "Ok I did that. What next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-15\", \"tracker_completed_step_num\": 35, \"tracker_requested_step\": \"inst-16\", \"tracker_requested_step_num\": 36}"
        },
        {
            "utt_id": "utt-59",
            "utterance": "Now gradually blend in the confectioners' sugar, alternating with 2 tablespoons vanilla and 2 tablespoons cream. Afterwards blend in the melted chocolate.",
            "bot": true,
            "annotations": "inform_instruction(inst-16);"
        },
        {
            "utt_id": "utt-60",
            "utterance": "Okay, I did that. What should I do with the mixture?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-16\", \"tracker_completed_step_num\": 36, \"tracker_requested_step\": \"inst-17\", \"tracker_requested_step_num\": 37}"
        },
        {
            "utt_id": "utt-61",
            "utterance": "Also beat in the softened cream cheese. Set aside. Now let's make the Ganache. Take a large saucepan and combine chocolate chips and 1 cup heavy cream.",
            "bot": true,
            "annotations": "inform_instruction(inst-17);\r\ninform_instruction(inst-18);"
        },
        {
            "utt_id": "utt-62",
            "utterance": "Ok, I love ganesh, how should I start?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-18\", \"tracker_completed_step_num\": 38, \"tracker_requested_step\": \"inst-19\", \"tracker_requested_step_num\": 39}"
        },
        {
            "utt_id": "utt-63",
            "utterance": "Then heat it and stir constantly, until the chocolate is melted and smooth.",
            "bot": true,
            "annotations": "inform_instruction(inst-19);"
        },
        {
            "utt_id": "utt-64",
            "utterance": "Okay the chocolate looks smooth. What is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-19\", \"tracker_completed_step_num\": 39, \"tracker_requested_step\": \"inst-20\", \"tracker_requested_step_num\": 40}"
        },
        {
            "utt_id": "utt-65",
            "utterance": "Now remove from heat and set aside. It is time to assemble the cake. Cover the bottom cake layer with as much ganache as desired.",
            "bot": true,
            "annotations": "inform_instruction(inst-20);\r\ninform_instruction(inst-21);"
        },
        {
            "utt_id": "utt-66",
            "utterance": "Ok there is ganesh on the bottom. What next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-21\", \"tracker_completed_step_num\": 41, \"tracker_requested_step\": \"inst-22\", \"tracker_requested_step_num\": 42}"
        },
        {
            "utt_id": "utt-67",
            "utterance": "Good now refrigerate for 5 minutes, or until the ganache has set.",
            "bot": true,
            "annotations": "inform_instruction(inst-22);"
        },
        {
            "utt_id": "utt-68",
            "utterance": "Ok I minutes past. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-22\", \"tracker_completed_step_num\": 42, \"tracker_requested_step\": \"inst-23\", \"tracker_requested_step_num\": 43}"
        },
        {
            "utt_id": "utt-69",
            "utterance": "Good now spread the frosting over the ganache. And cover with the top layer of cake.",
            "bot": true,
            "annotations": "inform_instruction(inst-23);\r\ninform_instruction(inst-24);"
        },
        {
            "utt_id": "utt-70",
            "utterance": "Okay, I did that. Is it ready now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_is_recipe_finished;\", \"tracker_completed_step\": \"inst-24\", \"tracker_completed_step_num\": 44, \"tracker_requested_step\": \"inst-25\", \"tracker_requested_step_num\": 45}"
        },
        {
            "utt_id": "utt-71",
            "utterance": "Good now frost the top and sides with buttercream and garnish with more ganache. That was all. I hope you enjoy your Elizabeths Extreme Chocolate Lovers Cake.",
            "bot": true,
            "annotations": "inform_instruction(inst-25);\r\nend_recipe();\r\nenjoy();"
        },
        {
            "utt_id": "utt-72",
            "utterance": "I love it!!! I love chocolate :-) thank you for the recipe bot.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank;\", \"tracker_completed_step\": \"inst-25\", \"tracker_completed_step_num\": 45, \"tracker_requested_step\": \"inst-25\", \"tracker_requested_step_num\": 45}"
        },
