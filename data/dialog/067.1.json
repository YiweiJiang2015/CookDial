{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hi. what are we making today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi. today we are making Easy Bake Oven Cookie Mix.",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);"
        },
        {
            "utt_id": "utt-2",
            "utterance": "How do I start?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_start;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Take a medium bowl in which you will have to combine 4 ingredients.",
            "bot": true,
            "annotations": "fetch(inst-0, [tool-0-0]);\r\nsimplify_begin(inst-0);"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Ok. I have the bowl. What are the ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "Start with 1 1/2 cups quick-cooking oats.",
            "bot": true,
            "annotations": "simplify_continue(inst-0, [ing-0]);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "Ok. what is the next one?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "Next add 3/4 cup all-purpose flour and 1/4 teaspoon baking soda.",
            "bot": true,
            "annotations": "simplify_continue(inst-0, [ing-1, ing-2]);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Ok. I have added both. What is the last one?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "The last one is 3/4 cup packed brown sugar.",
            "bot": true,
            "annotations": "simplify_continue(inst-0, [ing-3]);"
        },
        {
            "utt_id": "utt-10",
            "utterance": "Ok. what should I do with it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_instruction;\", \"tracker_completed_step\": \"ing-3\", \"tracker_completed_step_num\": 4, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 9}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "Stir everything well together.",
            "bot": true,
            "annotations": "simplify_begin(ac-0-0);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "Ok. I have done that. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 9, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "Now add 1/2 cup shortening to the mixture.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ok. I also added that. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 11}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "Make sure there are no lumps of shortening left.",
            "bot": true,
            "annotations": "inform_instruction(inst-2);"
        },
        {
            "utt_id": "utt-16",
            "utterance": "Ok. there are none left. Is there something else to do?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-2\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "Add 1/2 cup of the mixture into a re-sealable bag.",
            "bot": true,
            "annotations": "inform_instruction(inst-3);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Ok. I have done that. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "Do this until all of the mixture is consumed.",
            "bot": true,
            "annotations": "other();"
        },
        {
            "utt_id": "utt-20",
            "utterance": "Ok. all of the mixture is consumed now. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"inst-5\", \"tracker_requested_step_num\": 14}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "Empty one bag of the mixture into a bowl.",
            "bot": true,
            "annotations": "inform_instruction(inst-5);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Ok. I have done that. What is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-5\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 15}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "Now you should stir in 3 ingredients. Start with 2 teaspoons of water.",
            "bot": true,
            "annotations": "simplify_begin(inst-6);\r\nsimplify_continue(inst-6, [ing-5]);"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Ok. I have stirred the water in. What are the next ingredients?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 15}"
        },
        {
            "utt_id": "utt-25",
            "utterance": "The other two ingredients are 1 tablespoon of raisins and 1 tablespoon of mini semisweet chocolate chips.",
            "bot": true,
            "annotations": "simplify_continue(inst-6, [ing-6, ing-7]);"
        },
        {
            "utt_id": "utt-26",
            "utterance": "Ok. I have also added the two ingredients. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"inst-7\", \"tracker_requested_step_num\": 16}"
        },
        {
            "utt_id": "utt-27",
            "utterance": "Mix everything well and shape teaspoons of the mixture into small balls.",
            "bot": true,
            "annotations": "inform_instruction(inst-7);"
        },
        {
            "utt_id": "utt-28",
            "utterance": "Ok. I have shaped the mixture. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-7\", \"tracker_completed_step_num\": 16, \"tracker_requested_step\": \"inst-8\", \"tracker_requested_step_num\": 17}"
        },
        {
            "utt_id": "utt-29",
            "utterance": "Now you can place the balls on an ungreased cookie sheet.",
            "bot": true,
            "annotations": "inform_instruction(ac-8-0);"
        },
        {
            "utt_id": "utt-30",
            "utterance": "Ok. I have placed the balls. What is the next step?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-8\", \"tracker_completed_step_num\": 17, \"tracker_requested_step\": \"inst-8\", \"tracker_requested_step_num\": 17}"
        },
        {
            "utt_id": "utt-31",
            "utterance": "Next flatten the balls with bottom of a glass. The bottom of the glass should be buttered and dipped in sugar.",
            "bot": true,
            "annotations": "inform_instruction(ac-8-1);"
        },
        {
            "utt_id": "utt-32",
            "utterance": "Ok. I have done that. What should I do now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-8\", \"tracker_completed_step_num\": 17, \"tracker_requested_step\": \"inst-9\", \"tracker_requested_step_num\": 18}"
        },
        {
            "utt_id": "utt-33",
            "utterance": "Now you should follow your oven's instructions for baking the cookies. That's all. Enjoy your cookies!",
            "bot": true,
            "annotations": "inform_instruction(inst-9);\r\nend_recipe();\r\nenjoy();"
        },
        {
            "utt_id": "utt-34",
            "utterance": "Ok. I will do that. Thanks for the instructions.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank;\", \"tracker_completed_step\": \"inst-9\", \"tracker_completed_step_num\": 18, \"tracker_requested_step\": \"inst-9\", \"tracker_requested_step_num\": 18}"
        }
