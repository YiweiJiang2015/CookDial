{
    "messages": [
        {
            "utt_id": "utt-0",
            "utterance": "Hi, what do we do today?",
            "bot": false,
            "annotations": "{\"section\": \"title\", \"intent\": \"greeting; req_title;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"title\", \"tracker_requested_step_num\": 0}"
        },
        {
            "utt_id": "utt-1",
            "utterance": "Hi there, today we make a chicken soup!",
            "bot": true,
            "annotations": "greeting();\r\ninform_title(title);"
        },
        {
            "utt_id": "utt-2",
            "utterance": "Chicken soup. How should I start?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_start;\", \"tracker_completed_step\": \"title\", \"tracker_completed_step_num\": 0, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 11}"
        },
        {
            "utt_id": "utt-3",
            "utterance": "Start by heating 2 tablespoons of vegetable oil over medium heat in a large pot.",
            "bot": true,
            "annotations": "fetch(inst-0, [tool-0-0]);"
        },
        {
            "utt_id": "utt-4",
            "utterance": "Ok. The oil is hot now. What next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-0\", \"tracker_requested_step_num\": 11}"
        },
        {
            "utt_id": "utt-5",
            "utterance": "Cook 2 skinless chicken leg quarters until they are brown on both sides.",
            "bot": true,
            "annotations": "inform_instruction(inst-0);"
        },
        {
            "utt_id": "utt-6",
            "utterance": "The chicken are browned now. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-0\", \"tracker_completed_step_num\": 11, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-7",
            "utterance": "Chop 1/2 cup of onions and stir them into the pot. Cook for 2 minutes.",
            "bot": true,
            "annotations": "inform_instruction(inst-1);"
        },
        {
            "utt_id": "utt-8",
            "utterance": "Still with medium heat, right?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_temperature;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"inst-1\", \"tracker_requested_step_num\": 12}"
        },
        {
            "utt_id": "utt-9",
            "utterance": "Yes, medium heat is fine.",
            "bot": true,
            "annotations": "affirm();"
        },
        {
            "utt_id": "utt-10",
            "utterance": "2 minutes passed. Shall I add water into it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; other;\", \"tracker_completed_step\": \"inst-1\", \"tracker_completed_step_num\": 12, \"tracker_requested_step\": \"inst-2\", \"tracker_requested_step_num\": 13}"
        },
        {
            "utt_id": "utt-11",
            "utterance": "Yes, add water and chicken bouillon. Bring it to a boil.",
            "bot": true,
            "annotations": "affirm();\r\ninform_instruction(inst-2);"
        },
        {
            "utt_id": "utt-12",
            "utterance": "The water boils now. What else to do?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-2\", \"tracker_completed_step_num\": 13, \"tracker_requested_step\": \"inst-3\", \"tracker_requested_step_num\": 14}"
        },
        {
            "utt_id": "utt-13",
            "utterance": "Reduce the heat and let it simmer for 45 minutes.",
            "bot": true,
            "annotations": "inform_instruction(inst-3);"
        },
        {
            "utt_id": "utt-14",
            "utterance": "Ohh. I need to find my lid. Ok. I let it simmer for 45 minutes.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm;\", \"tracker_completed_step\": \"inst-3\", \"tracker_completed_step_num\": 14, \"tracker_requested_step\": \"inst-4\", \"tracker_requested_step_num\": 15}"
        },
        {
            "utt_id": "utt-15",
            "utterance": "Chop 1 stalk of celery and 3 carrots. Mince 1 clove of roasted garlic.",
            "bot": true,
            "annotations": "simplify_continue(inst-4, [ing-5, ing-6, ing-7]);"
        },
        {
            "utt_id": "utt-16",
            "utterance": "I shall add these to the pot, right?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"other;\", \"tracker_completed_step\": \"inst-4\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"inst-4\", \"tracker_requested_step_num\": 15}"
        },
        {
            "utt_id": "utt-17",
            "utterance": "Yes, also stir in some salt and pepper.",
            "bot": true,
            "annotations": "affirm();\r\nsimplify_continue(inst-4, [ing-8]);"
        },
        {
            "utt_id": "utt-18",
            "utterance": "Ok. Done. How long to wait from now?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_duration;\", \"tracker_completed_step\": \"inst-4\", \"tracker_completed_step_num\": 15, \"tracker_requested_step\": \"inst-5\", \"tracker_requested_step_num\": 16}"
        },
        {
            "utt_id": "utt-19",
            "utterance": "Wait until the carrots are tender. Then remove the chicken pieces from the soup.",
            "bot": true,
            "annotations": "inform_instruction(inst-5);\r\ninform_instruction(ac-6-0);"
        },
        {
            "utt_id": "utt-20",
            "utterance": "The chicken are out of the pot. What is next?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_instruction;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 17, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 17}"
        },
        {
            "utt_id": "utt-21",
            "utterance": "Pull the meat from the bone.",
            "bot": true,
            "annotations": "inform_instruction(ac-6-1);"
        },
        {
            "utt_id": "utt-22",
            "utterance": "Pour the meat?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"other;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 17, \"tracker_requested_step\": \"inst-6\", \"tracker_requested_step_num\": 17}"
        },
        {
            "utt_id": "utt-23",
            "utterance": "No, remove the meat from the bone.",
            "bot": true,
            "annotations": "negate();\r\nrepeat(ac-6-1);"
        },
        {
            "utt_id": "utt-24",
            "utterance": "Thanks! I remove the bones.",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank; confirm;\", \"tracker_completed_step\": \"inst-6\", \"tracker_completed_step_num\": 17, \"tracker_requested_step\": \"inst-7\", \"tracker_requested_step_num\": 18}"
        },
        {
            "utt_id": "utt-25",
            "utterance": "Continue by stirring the noodles into the pot.",
            "bot": true,
            "annotations": "inform_instruction(inst-7);"
        },
        {
            "utt_id": "utt-26",
            "utterance": "What is the amount of the noodles?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"req_amount;\", \"tracker_completed_step\": \"inst-7\", \"tracker_completed_step_num\": 18, \"tracker_requested_step\": \"ing-9\", \"tracker_requested_step_num\": 10}"
        },
        {
            "utt_id": "utt-27",
            "utterance": "1 package containing 12 ounce of thin egg noodles.",
            "bot": true,
            "annotations": "inform_amount(ing-9);"
        },
        {
            "utt_id": "utt-28",
            "utterance": "Thanks. I add it. How long shall I cook the noodles?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank; confirm; req_duration;\", \"tracker_completed_step\": \"ing-9\", \"tracker_completed_step_num\": 10, \"tracker_requested_step\": \"inst-8\", \"tracker_requested_step_num\": 19}"
        },
        {
            "utt_id": "utt-29",
            "utterance": "When the noodles are tender, you can return the chicken to the pot.",
            "bot": true,
            "annotations": "inform_instruction(inst-8);"
        },
        {
            "utt_id": "utt-30",
            "utterance": "Great. The chicken are added back. Can I eat it?",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"confirm; req_is_recipe_finished;\", \"tracker_completed_step\": \"inst-8\", \"tracker_completed_step_num\": 19, \"tracker_requested_step\": \"inst-8\", \"tracker_requested_step_num\": 19}"
        },
        {
            "utt_id": "utt-31",
            "utterance": "Yes, the chicken soup is ready to be served. Enjoy!",
            "bot": true,
            "annotations": "affirm();\r\nenjoy();"
        },
        {
            "utt_id": "utt-32",
            "utterance": "Thanks!",
            "bot": false,
            "annotations": "{\"section\": \"inst\", \"intent\": \"thank;\", \"tracker_completed_step\": \"inst-8\", \"tracker_completed_step_num\": 19, \"tracker_requested_step\": \"inst-8\", \"tracker_requested_step_num\": 19}"
        },
        {
            "utt_id": "utt-33",
            "utterance": "My pleasure.",
            "bot": true,
            "annotations": "thank();"
        }
    ]
}